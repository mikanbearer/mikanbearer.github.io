{{ define "result" }}
[
{{ range $i, $e := where site.RegularPages "Type" "in" "works" }}
    {{ if $i }}, {{ end }}
        {
            "title": "{{ $e.Title }}",
            "summary": "{{ $e.Summary }}",
            "images": [
                {{ range $k, $v := $e.Params.images }}
                    {{ if $k }}, {{ end }}
                    "{{ $e.Permalink }}{{ $v }}"
                {{ end }}
            ],
            "powered_by": [
                {{ range $k, $v := $e.Params.powered_by }}
                    {{ if $k }}, {{ end }}
                    "{{ $v }}"
                {{ end }}
            ],
            "download_url": "{{ $e.Params.download_url }}",
            "demo_url": "{{ $e.Params.demo_url }}"
        }
{{ end }}
]
{{ end }}