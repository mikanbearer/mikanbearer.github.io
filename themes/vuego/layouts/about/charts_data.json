{{ define "result" }}
  { 
    "monthly_count": {
      "x_data": [
        {{ range $i, $e := (where site.RegularPages "Type" "in" site.Params.mainSections).GroupByDate "2006-01" }}
          {{ if $i }},{{ end }}"{{ $e.Key }}"
        {{ end }}
      ],
      "series_data": [
        {{ range $i, $e := (where site.RegularPages "Type" "in" site.Params.mainSections).GroupByDate "2006-01" }}
          {{ if $i }},{{ end }}"{{ len $e.Pages }}"
        {{ end }}
      ]
    },

    {{ $taxonomy := .Site.Taxonomies.categories }}
    "categories_data": {
      "legendData": [
        {{ range $i, $e := $taxonomy.ByCount }}
          {{ if $i }},{{ end }}"{{ $e.Name }}"
        {{ end }}
      ],
  
      "seriesData": [
        {{ range $i, $e := $taxonomy.ByCount }}
          {{ if $i }},{{ end }}{
            "name": "{{ $e.Name }}",
            "value": {{ $e.Count }}
          }
        {{ end }}
      ],

      "selected": {
        {{ range $i, $e := $taxonomy.ByCount }}
          {{ if $i }},{{ end }}"{{ $e.Name }}": 
          {{ if ge $i 10 }}false
          {{ else }}true
          {{ end }}
        {{ end }}
      }
    },
  
    {{ $taxonomy := .Site.Taxonomies.tags }}
    "tags_data": {
      "legendData": [
        {{ range $i, $e := $taxonomy.ByCount }}
          {{ if $i }},{{ end }}"{{ $e.Name }}"
        {{ end }}
      ],
  
      "seriesData": [
        {{ range $i, $e := $taxonomy.ByCount }}
          {{ if $i }},{{ end }}{
            "name": "{{ $e.Name }}",
            "value": {{ $e.Count }}
          }
        {{ end }}
      ],

      "selected": {
        {{ range $i, $e := $taxonomy.ByCount }}
          {{ if $i }},{{ end }}"{{ $e.Name }}": 
          {{ if ge $i 10 }}false
          {{ else }}true
          {{ end }}
        {{ end }}
      }
    }
  }

{{ end }}