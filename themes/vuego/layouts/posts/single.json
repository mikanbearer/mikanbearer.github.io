{{ define "result" }}
{
    "title": "{{ .Title }}",
    "date": "{{ .Date.Format "2006-01-02 15:04" }}",
    "draft": "{{ .Draft }}",
    "categories": 
    [
        {{ range $i, $e := .Params.Categories }}
            {{ if $i }}, {{ end }}"{{ $e }}"
        {{ end }}
    ],
    "tags":
    [
        {{ range $i, $e := .Params.Tags }}
            {{ if $i }}, {{ end }}"{{ $e }}"
        {{ end }}
    ],

    "url": "{{ .Permalink }}"
}
{{ end }}